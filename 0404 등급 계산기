#include <stdio.h>

int main()
{

	int a = 0;

	scanf_s("%d", &a);

	if (a > 0 && a % 1 == 0) {
	    if (a >= 90) {
		    a = a * 1.1;
		    printf("A등급! 점수: %d \n", a);
	    }
	    else if (a >= 80) {
		    a = a * 1.05;
		    printf("B등급! 점수: %d \n", a);
	    }
	    else if (a >= 70) {
	    	printf("C등급! 점수: %d \n", a);
	    }
	    else if (a >= 60) {
	     	printf("D등급! 점수: %d \n", a);
	    }
	    else {
		    printf("F등급! 점수: %d \n", a);
	    }
	}
	else
		printf("퇴학\n");
	return 0;
}
