#include <stdio.h>

int main()
{

	double a = 0;

	scanf_s("%lf", &a);

	
		if (a >= 90) {
			a = a * 1.1;
			printf("A등급! 점수: %lf \n", a);
		}
		else if (a >= 80) {
			a = a * 1.05;
			printf("B등급! 점수: %lf \n", a);
		}
		else if (a >= 70) {
			printf("C등급! 점수: %lf \n", a);
		}
		else if (a >= 60) {
			printf("D등급! 점수: %lf \n", a);
		}
		else {
			printf("F등급! 점수: %lf \n", a);
		}
	
	return 0;
}
