#include <stdio.h>

int main()
{
    int a;
    scanf_s("%d", &a);

    if (a > 0) {
        for (int i = 0; i < a; i++) {
            for (int j = a; j > i; j--) {
                printf("*");
            }
            printf("\n");
        }
    } else {
        printf("0보다 큰 수 입력!");
    }

    return 0;
}
