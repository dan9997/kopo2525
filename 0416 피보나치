#include <stdio.h>

int main()
{
    pibo();
    
    return 0;
}

void pibo(){
    printf("Hello World");
    int k;
    int a = 0, b = 1, temp, sum = 0;
    int one = 0, two = 0;
    scanf("%d", &k);
    
    while(b <= k){
        temp = a + b;
        a = b;
        b = temp;
        if(a % 2 == 0){
            two++;
        } else {
            one++;
        }
        sum = sum + a;
        printf("%d \n", a);
    }
    printf("총 합: %d 홀수: %d, 짝수: %d\n",sum, one, two);
}
