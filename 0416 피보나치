#include <stdio.h>

int main()
{
    int k;
    printf("입력 값(최대 값) : ");
    scanf("%d", &k);
    pibo(k);
    return 0;
}

void pibo(int k){
    int a = 0, b = 1, temp, sum = 0;
    int one = 0, two = 0;
    printf("출력 값\n");
    while(b <= k){
        temp = a + b;
        a = b;
        b = temp;
        if(a % 2 == 0){
            two++;
        } else {
            one++;
        }
        sum = sum + a;
        printf("%d ", a);
    }
    
    
    printf("\n합 : %d \n홀/짝 : %d/%d\n",sum, one, two);
}
