#include <stdio.h>
#include <stdlib.h>
#include <time.h>
void main()
{
    int temp = 0, temp2 = 0;
    int num[6];

    srand(time(NULL));

    for (int i = 0; i < 6; i++) {
        num[i] = (rand() % 45) + 1;
        printf("%d ", num[i]);
        for (int j = 0; j < i; j++) {
            if (num[j] == num[i]){
                i--;
            }
        }
    }
    
    printf("\n");

    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < i + 1 ; j++) {
            if (num[j] > num[i + 1]) {
                temp = num[j];
                num[j] = num[i + 1];
                num[i + 1] = temp;
            }  
        }
        for (int i = 0; i < 6; i++){
        printf("%d ", num[i]);
        }
        printf("\n");
    }
    printf("결과 :");
    for (int i = 0; i < 6 ; i++) {
        printf("%d ", num[i]);
    }

}
