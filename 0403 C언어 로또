#include <stdio.h>
#include <stdlib.h>
#include <time.h>
void main()
{		
int num2 = 99, temp = 0, temp2 = 0;
    double num[6];
    
    srand(time(NULL));
    
    for (int i = 0; i < 6; i++){
        num[i] = (rand() % 45) + 1;
        printf("%d\n",(int)num[i]);
        
        for(int j = 0; j < i; j++){
            if(num[j] == num[i] && (num[j ] != 0))
                {i = i - 1;}
        }
    sleep(1);
    }
    
    for(int i = 0; i < 6; i++){
        for(int j = 0; j < 6; j++){
            if(num[i]<num[j]){
                temp2 = num[j];
                num[j] = num[i];
                num[i] = temp2;
            }
        }
    }
    
    for(int i = 0; i < 6; i++){
        printf("%d ", (int)num[i]);
    }

}
