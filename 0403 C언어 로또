#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void main()
{		
    int temp = 0;
    int num[6];
    
    srand(time(NULL));
    
    for (int i = 0; i < 6; i++) {
        num[i] = (rand() % 45) + 1;
        printf("%d\n", num[i]);
        for (int j = 0; j < i; j++) {
            if (num[j] == num[i]) {
                i--;
            }
        }
        sleep(1);
    }
    
    for (int i = 0; i < 5; i++) {
        for (int j = i + 1; j < 6; j++) {
            if (num[i] > num[j]) {
                temp = num[j];
                num[j] = num[i];
                num[i] = temp;
            }
        }
    }
    
    for( int i = 0; i < 6; i++){
        printf("%d ", num[i]);
    }
}
