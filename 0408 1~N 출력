#include <stdio.h>
int main()
{
    int a, i = 1;
    scanf("%d", &a);
    printf("\n\n");
    while (i <= a) {
        printf("%d \n", i);
        i++;
        
    }
    printf("\n\n\n");
    
    scanf("%d", &a);
    i = 1;

    while (i <= a) {
        if (i % 2 == 0) {
            printf("%d \n", i);
        }
        i++;
    }

    return 0;
}
